
@{
    ViewBag.Title = "insonguyento";
}

<h2>In ra số nguyên tố nhỏ hơn 100</h2>
@{
    int i;
    for (i = 2; i < 100; i++)
    {
        bool snt = true;
        for (int j = 2; j <= Math.Sqrt(i); j++)
        {
            if (i % j == 0)
            {
                snt = false;
                break;
            }
        }

        if (snt)
        {
            @i
            <br />
        }
    }
}

